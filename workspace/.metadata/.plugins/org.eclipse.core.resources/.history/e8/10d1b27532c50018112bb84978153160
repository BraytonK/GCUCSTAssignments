        fah = input.nextDouble();
        System.out.println("The sum of the digits is: " + fah);
        System.out.print("Enter a Fahrenheit temperature: ");
        fah = input.nextDouble();
        System.out.println("The sum of the digits is: " + cel);
    }
	
