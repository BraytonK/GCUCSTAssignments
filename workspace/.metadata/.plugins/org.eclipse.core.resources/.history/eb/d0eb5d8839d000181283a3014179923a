import java.io.File;
import java.util.Scanner;
public class main {
	public static void main(String args[]) throws Exception
    {
		StringBuilder sb = new StringBuilder();
		File file = new File("C:\\Users\\kerek\\Desktop\\project5.txt"); 
		Scanner input = new Scanner(file);
		
		int[] dimensions = {3,5};
		String[][] stringArr = new String[dimensions[0]][dimensions[1]];
		int counter = 0;
		int dimensionCount = 0;
		int dimensionParameter = 0;
		int maxDimensions = dimensions[0] - 1;
		
		while(input.hasNext()){
			String read = input.next();
			char[] inputCharArrConverter = read.toCharArray();
			for(char inputOut : inputCharArrConverter){
				if(counter < dimensions[1]){
					//System.out.println(String.valueOf(inputOut));
					//System.out.println("dimension count: " + dimensionCount);
					//System.out.println("counter: " + counter);
					stringArr[dimensionCount][counter] = String.valueOf(inputOut);
					//System.out.print(stringArr[dimensionCount][counter]);
					counter++;
					}
				else{
					if(dimensionCount < maxDimensions){
						//System.out.println("max dimensions " + maxDimensions);
						dimensionCount++;
						counter = 0;
						stringArr[dimensionCount][counter] = String.valueOf(inputOut);
						//System.out.print(stringArr[dimensionCount][counter]);
						}
					else{
						//System.out.println("out!");
						break;
						}
					}
				}
			if(counter < dimensions[1]){
				stringArr[dimensionCount][counter] = "@";
				counter++;
			}
		}
		dimensionCount = 0;
		counter = 0;
		//System.out.println(stringArr[1][1]);
		while(dimensionCount <= maxDimensions){
			System.out.println("dimension count: " + dimensionCount);
			System.out.println("counter: " + counter);
			if(counter < dimensions[1]){
				System.out.println(stringArr[dimensionCount][counter]);
				counter++;
			}
			//System.out.println(dimensionCount + " " + counter);
			else{
				dimensionCount++;
				counter = 0;
			}
			//counter++;
		}
    }
}
